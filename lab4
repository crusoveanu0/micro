int main(){
unsigned char inputs;
DDRA=0xff;
DDRB=0;
inputs = PINB & 0b00000111;
switch(imputs){
  case 0:
    PORTA = 0b0000001;
  case 1:
      PORTA = 0b1 0 0 1 1 1 1;
      break;
  case 2:
      PORTA = 0b0 1 0 1 0 0 1;
      break;
  case 3;
        PORTA = 0b0 0 0 0 1 1 0;
        break;
  case 4:
        PORTA = 0b0 1 1 0 0 1 1;
        break;
  case 5:
        PORTA = 0b0 1 0 0 1 0 0;
        break;
  case 6:
        PORTA = 0b0 1 0 0 0 0 0;
